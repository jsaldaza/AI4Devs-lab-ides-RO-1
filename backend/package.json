{
  "name": "backend",
  "version": "1.0.0",
  "description": "Backend for candidates application",
  "main": "dist/infrastructure/index.js",
  "scripts": {
    "start": "node --no-deprecation dist/infrastructure/index.js",
    "dev": "ts-node-dev --no-deprecation --respawn --transpile-only src/infrastructure/index.ts",
    "build": "tsc",
    "typeorm": "typeorm-ts-node-commonjs",
    "migration:generate": "npm run typeorm migration:generate",
    "migration:run": "npm run typeorm migration:run",
    "test": "jest",
    "test:watch": "jest --watch",
    "test:coverage": "jest --coverage",
    "test:setup-db": "ts-node scripts/setup-test-db.ts",
    "test:integration": "jest --config jest.config.js --testMatch='**/*.test.ts' --runInBand",
    "test:unit": "jest --config jest.config.js --testMatch='**/__tests__/**/*.test.ts'"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "class-transformer": "^0.5.1",
    "class-validator": "^0.14.0",
    "cors": "^2.8.5",
    "dotenv": "^16.3.1",
    "express": "^4.18.2",
    "jsonwebtoken": "^9.0.2",
    "pg": "^8.11.3",
    "reflect-metadata": "^0.1.13",
    "typeorm": "^0.3.17"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.6",
    "@types/cors": "^2.8.17",
    "@types/express": "^4.17.21",
    "@types/jest": "^29.5.14",
    "@types/jsonwebtoken": "^9.0.5",
    "@types/node": "^20.10.5",
    "@types/supertest": "^6.0.3",
    "jest": "^29.7.0",
    "sqlite3": "^5.1.7",
    "supertest": "^6.3.4",
    "ts-jest": "^29.3.4",
    "ts-node-dev": "^2.0.0",
    "typescript": "^5.3.3"
  }
}
